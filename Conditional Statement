#include<stdio.h>
int main()
{
int a,marks;
printf("Enter your marks");
scanf("%d",&marks);
if(a<85&&a>=100)
{
printf("grade a");
}
else if (a<74&&a>=84)
{
printf("Grade B");
else if (a<55 && a>= 69)
{
printf("Grade C");
}
else if (a<40 && a>= 54 )
{
printf(" Grade D");
}
else (a<40)
{
printf("Grade F");
}
}


